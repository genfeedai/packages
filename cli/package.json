{
  "author": "Genfeed.ai",
  "bin": {
    "gf": "dist/index.js",
    "genfeed": "dist/index.js"
  },
  "dependencies": {
    "@inquirer/prompts": "8.2.0",
    "chalk": "5.6.2",
    "commander": "14.0.3",
    "ofetch": "1.5.1",
    "ora": "9.3.0",
    "socket.io-client": "4.8.3",
    "zod": "4.3.6"
  },
  "description": "Unified CLI for Genfeed.ai - Generate, train, publish, and manage AI content from your terminal",
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@types/bun": "1.3.9",
    "@types/node": "25.2.3",
    "@vitest/coverage-v8": "4.0.18",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://genfeed.ai",
  "keywords": [
    "genfeed",
    "ai",
    "image-generation",
    "video-generation",
    "training",
    "darkroom",
    "cli"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@genfeedai/cli",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genfeedai/packages.git",
    "directory": "cli"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "build:standalone": "bun build src/index.ts --compile --outfile gf",
    "dev": "bun run src/index.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "deps:update": "bunx npm-check-updates -u && bun install",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "version": "0.2.0"
}
