{
  "name": "@genfeedai/workflow-ui",
  "version": "0.2.0",
  "type": "module",
  "license": "AGPL-3.0",
  "description": "Shared UI components for Genfeed workflow editor",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genfeedai/packages.git",
    "directory": "workflow-ui"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./canvas": {
      "types": "./dist/canvas.d.ts",
      "import": "./dist/canvas.mjs"
    },
    "./nodes": {
      "types": "./dist/nodes.d.ts",
      "import": "./dist/nodes.mjs"
    },
    "./panels": {
      "types": "./dist/panels.d.ts",
      "import": "./dist/panels.mjs"
    },
    "./toolbar": {
      "types": "./dist/toolbar.d.ts",
      "import": "./dist/toolbar.mjs"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "import": "./dist/hooks.mjs"
    },
    "./stores": {
      "types": "./dist/stores.d.ts",
      "import": "./dist/stores.mjs"
    },
    "./provider": {
      "types": "./dist/provider.d.ts",
      "import": "./dist/provider.mjs"
    },
    "./ui": {
      "types": "./dist/ui.d.ts",
      "import": "./dist/ui.mjs"
    },
    "./lib": {
      "types": "./dist/lib.d.ts",
      "import": "./dist/lib.mjs"
    },
    "./styles": "./src/styles/workflow-ui.css"
  },
  "files": [
    "dist",
    "src/styles"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "@xyflow/react": ">=12.0.0",
    "next": ">=14.0.0",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0",
    "zustand": ">=5.0.0",
    "zundo": ">=2.0.0"
  },
  "dependencies": {
    "@dagrejs/dagre": "^2.0.4",
    "@genfeedai/types": "workspace:*",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "nanoid": "^5.1.6",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@genfeedai/core": "workspace:*",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/node": "^25.2.3",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "@xyflow/react": "12.10.0",
    "jsdom": "26.0.0",
    "react": "19.2.4",
    "react-compare-slider": "^3.1.0",
    "react-dom": "19.2.4",
    "tsup": "8.4.0",
    "typescript": "5.9.3",
    "vitest": "3.0.4",
    "zundo": "2.3.0",
    "zustand": "5.0.3"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "bun run build",
    "test": "vitest run"
  }
}
